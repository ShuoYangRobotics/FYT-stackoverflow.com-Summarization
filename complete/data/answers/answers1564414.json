[[{"text": ["Here you go"], "childNum": 0, "tag": "p", "senID": 0, "childList": []}, {"code": "<pre>\n<code>\n myString = 'ABCDEFGHIJKLM'\nfirst = myString[3:7] # =&gt; DEFG\nsecond = myString[8:11] # =&gt; IJK\n</code>\n</pre>\n", "senID": 1}, {"text": ["In the slicing syntax, the first number is inclusive and the second is excluded."], "childNum": 0, "tag": "p", "senID": 2, "childList": []}, {"text": ["You can read more about String slicing from python docs"], "childNum": 1, "tag": "p", "senID": 3, "childList": [{"text": "python docs", "tag": "a", "pos": 0, "childList": [], "childNum": 0, "href": "http://docs.python.org/tutorial/introduction.html#strings"}]}], [{"text": ["Look into Python's concept called sequence slicing!"], "childNum": 0, "tag": "p", "senID": 0, "childList": []}], [{"code": "<pre>\n<code>\n a = \"ABCDEFGHIJKLM\"\nprint a[3:7], a[8:11]\n</code>\n</pre>\n", "senID": 0}, {"text": ["--> DEFG IJK"], "childNum": 0, "tag": "p", "senID": 1, "childList": []}], [{"code": "<pre>\n<code>\n s = 'ABCDEFGHIJKLM'\nprint s[3:7]\nprint s[8:11]\n</code>\n</pre>\n", "senID": 0}], [{"code": "<pre>\n<code>\n &gt;&gt;&gt; 'ABCDEFGHIJKLM'[3:7]\n'DEFG'\n&gt;&gt;&gt; 'ABCDEFGHIJKLM'[8:11]\n'IJK'\n</code>\n</pre>\n", "senID": 0}, {"text": ["You might want to read a tutorial or beginners book."], "childNum": 1, "tag": "p", "senID": 1, "childList": [{"text": "tutorial", "tag": "a", "pos": 0, "childList": [], "childNum": 0, "href": "http://docs.python.org/tutorial/"}]}], [{"text": ["In alternative you can use operator.itemgetter:"], "childNum": 0, "tag": "p", "senID": 0, "childList": []}, {"code": "<pre>\n<code>\n &gt;&gt;&gt; import operator\n&gt;&gt;&gt; s = 'ABCDEFGHIJKLM'\n&gt;&gt;&gt; f = operator.itemgetter(3,4,5,6,7,8,9,10,11)\n&gt;&gt;&gt; f(s)\n('D', 'E', 'F', 'G', 'H', 'I', 'J', 'K', 'L')\n</code>\n</pre>\n", "senID": 1}]]